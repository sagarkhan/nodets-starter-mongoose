{
  "name": "nodets-starter-mongoose",
  "version": "2.0.0",
  "description": "NodeJS boilerplate with typescript support, airbnb lint and prettier configured, MongoDB database, Swagger configured",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/sagarkhan/nodets-starter-mongoose.git"
  },
  "scripts": {
    "start": "node dist/index.js | pino-pretty",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "precise-commits": "precise-commits  --whitelist \"**/server/**/*\" --check-only",
    "husky": "npm run lint && npm run precise-commits",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.31",
    "mongoose": "^6.0.3",
    "mongoose-lean-virtuals": "^0.8.0",
    "multer": "^1.4.2",
    "pino": "^6.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.7",
    "@types/mocha": "^9.0.0",
    "@types/pino": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.4",
    "pino-pretty": "^6.0.0",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.2"
  },
  "author": "Sagar Khan <sk.sagarkhan95@gmail.com> "
}
